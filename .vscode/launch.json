{
  "version": "0.2.0",
  "configurations": [
    {
      "request": "launch",
      "name": "Debug (DEVT)",
      "type": "node",
      "program": "${workspaceFolder}/main.ts",
      "cwd": "${workspaceFolder}",
      "env": {},
      "runtimeExecutable": "/opt/homebrew/bin/deno",
      "runtimeArgs": [
        "run",
        "--unstable",
        "--inspect-wait",
        "--allow-all",
        "source/main.ts"
      ],
      "attachSimplePort": 9229
    },
    {
      "request": "launch",
      "name": "Debug (PROD)",
      "type": "node",
      "program": "${workspaceFolder}/main.ts",
      "cwd": "${workspaceFolder}",
      "env": {},
      "runtimeExecutable": "/opt/homebrew/bin/deno",
      "runtimeArgs": [
        "run",
        "--unstable",
        "--inspect-wait",
        "--allow-all",
        "source/main.ts",
        "--production"
      ],
      "attachSimplePort": 9229
    }
  ]
}
